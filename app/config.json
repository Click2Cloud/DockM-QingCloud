    {
      "key": "env",
      "description": "application configuration properties",
      "type": "array",
      "properties": [
        {
          "key": "access_key_id",
          "label": "Access key ID",
          "description": "API access key ID",
          "type": "string",
          "pattern": "^[A-Z]{20}$",
          "required": "yes"
        },
        {
          "key": "secret_access_key",
          "label": "Secret access key",
          "description": "API secret access key",
          "type": "password",
          "pattern": "^[A-Za-z0-9]{40}$",
          "required": "yes"
        },
        {
          "key": "registry-mirrors",
          "label": "Registry mirrors",
          "description": "The docker hub registry mirror, use a blank to split multi registry",
          "type": "string",
          "default": "https://registry.docker-cn.com",
          "required": "no"
        },
        {
          "key": "insecure-registries",
          "label": "Insecure registries",
          "description": "The insecure docker registries, use a blank to split multi registry",
          "type": "string",
          "default": "",
          "required": "no"
        },
        {
          "key": "dockerhub_username",
          "label": "Dockerhub Username",
          "description": "The username of dockerhub.qingcloud.com",
          "type": "string",
          "default": "",
          "changeable": true,
          "required": "no"
        },
        {
          "key": "enable_hostnic",
          "label": "Enable host nic",
          "description": "Switch to open host nic",
          "type": "boolean",
          "changeable": false,
          "default": false,
          "required": "yes"
        },
        {
          "key": "dockerhub_password",
          "label": "Docker hub Password",
          "description": "The password of dockerhub.qingcloud.com user",
          "type": "password",
          "default": "",
          "changeable": true,
          "required": "no"
        },
        {
          "key": "keep_log_days",
          "label": "Keep log days",
          "description": "Keep log on log node for x days, 0 indicates that the log is not cleared",
          "type": "integer",
          "default": 3,
          "min": 0,
          "max": 30,
          "changeable": true,
          "required": "yes"
        },
        {
          "key": "dockm_log_level",
          "label": "DockM log level",
          "description": "The log level for DockM system",
          "type": "integer",
          "default": 0,
          "min": 0,
          "max": 10,
          "changeable": true,
          "required": "no"
        },
        {
          "key": "fluent_forward_server",
          "label": "Fluent forward server",
          "description": "The fluent log server address to forward server, format host:port",
          "type": "string",
          "changeable": true,
          "default": "",
          "pattern": "^$|^[^\\:]+:[0-9]{1,5}$",
          "required": "no"
        },
        {
          "key": "es_server",
          "label": "Elastic Search server",
          "description": "The Elastic Search server address, format host:port",
          "type": "string",
          "changeable": true,
          "default": "",
          "pattern": "^$|^[^\\:]+:[0-9]{1,5}$",
          "required": "no"
        },
        {
          "key": "enable_istio",
          "label": "Enable istio",
          "description": "Enable service mesh platform istio",
          "type": "string",
          "changeable": true,
          "default": "no",
          "range": [
            "yes",
            "no"
          ],
          "required": "no"
        }
      ]
    }
  ]
